  <script type="text/javascript">
  const resultElem = document.getElementById("result");
  resultElem.style.display = "none";

  const notFoundElem = document.getElementById("not-found");
  notFoundElem.style.display = "none";

  const loadingElem = document.getElementById("loading");
  loadingElem.style.display = "none";

  const errorElem = document.getElementById("error");
  errorElem.style.display = "none";

  const curlEmail = document.getElementById("curl-email");

  const handle = async (e) => {
    e.preventDefault();

    const jobTitleElem = document.getElementById("occupation");
    const locationElem = document.getElementById("location");

    const data = new URLSearchParams();
    data.append("occupation", jobTitleElem.value.replace(/\/$/, ""));
    data.append("location", locationElem.value.replace(/\/$/, ""));

    loadingElem.style.display = "inline-block";
    errorElem.style.display = "none";
    resultElem.style.display = "none";
    notFoundElem.style.display = "none";

    const resp = await fetch("{{ url_for('linkedin_xray_tool_post') }}", {
      method: "POST",
      body: data,
    });


    loadingElem.style.display = "none";

    if (!resp.ok) {
      errorElem.innerHTML = await resp.text();
      errorElem.style.display = "block";
      return;
    }

    const body = await resp.json()

    document.getElementById("job_title").innerHTML = jobTitleElem.value;
    document.getElementById("location_param").innerHTML = locationElem.value;

    const matched_profiles_elem = document.getElementById("matched_profiles");

    body.profiles.forEach(profile => {
    matched_profiles_elem.innerHTML += `<div class="d-flex card card-body border border-gray-300 shadow-light-lg mb-6">
    <div class="d-flex w-100">
    <div style="width: 100px;" class="mr-4">
      <img src="${profile.profile_pic_url}" alt="profile picture" class="img-fluid rounded mb-2 d-none d-lg-block rounded-circle mr-4" >
    </div>
    <div class="mt-2">
      <span class="d-inline font-weight-bold">${profile.full_name}</span> <a href="${profile.linkedin_profile_url}" target="_blank noopener noreferrer"><i class="h4 neu-br-linkedin-sq"></i></a>
      <span class="d-block">${profile.occupation}</span>
      <span class="d-block text-muted">${profile.country}</span>
      <span class="text-muted d-block mt-4">40+ more data points</span>
      <button class="mt-2 btn btn-sm btn-primary-soft">See all with Proxycurl API</button>
    </div>

    </div>
    </div>`;
});


    if (body.full_name === null) {
      notFoundElem.style.display = "block";
      return;
    }


    resultElem.style.display = "block";
  };

  document.addEventListener("DOMContentLoaded", function () {
    const form = document.getElementById("form");
    form.addEventListener("submit", handle);
  });
  </script>
{% endblock loadlater %}
