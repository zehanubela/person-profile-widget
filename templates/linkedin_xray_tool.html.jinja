// Toggle the expand/show of the Proxycurl's JSON output component
  window.toggleJSONHeight = function toggleJSONHeight(elementId) {
      var jsonDiv = document.getElementById(elementId);
      // If the max height is not set to "none", it will set it to "none" to hide the content.
      if (jsonDiv.style.maxHeight != "none") {
          jsonDiv.style.maxHeight = "none";
      } else {
          // Otherwise, it will set it to "180px" to show the content.
          jsonDiv.style.maxHeight = "180px";
      }
  }

  // looping because there can be multiple recaptcha forms on the page
  window.captchaCallbackx = function captchaCallbackx() {
        document.getElementById('recaptcha-btnx').removeAttribute('disabled');
    };
  window.onLoadx = function onLoadx() {
      document.getElementById('recaptcha-spinnerx').style.display = 'none';
  }
  </script>
  <script src="https://www.google.com/recaptcha/api.js?onload=onLoadx"
          async
